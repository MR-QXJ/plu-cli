  "description": "frontend cli tool for development",
  "main": "bin/index.js",
  "bin": {
    "he": "bin/index.js"
  },
  "scripts": {
    "build": "node ./build && tsc",
    "start": "node ./build && tsc --watch",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix"
