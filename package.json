    "ejs": "^3.1.6",
    "figlet": "^1.5.0",
    "inquirer": "^8.1.2",
    "module-alias": "^2.2.2",
    "ora": "^5.4.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.7",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^14.17.6",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
